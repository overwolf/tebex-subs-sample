{
  "manifest_version": "1",
  "type": "WebApp",
  "permissions": ["Extensions", "Hotkeys", "GameInfo", "FileSystem"],
  "meta": {
    "name": "Overwolf Tebex Subscriptions Sample App",
    "version": "0.1.0",
    "minimum-overwolf-version": "0.220.0.1",
    "author": "Overwolf",
    "icon": "assets/icon.png",
    "icon_gray": "assets/icon_gray.png",
    "launcher_icon": "assets/desktop_icon.ico",
    "description": "Game Events Consumer Sample"
  },
  "data": {
    "start_window": "index",
    "windows": {
      "index": {
        "open_dev_tools": true,
        "file": "windows/index/page.html",
        "native_window": true,
        "desktop_only": true,
        "dev_tools_window_style": false,
        "resizable": true
      }
    },
    "developer": {
      "enable_auto_refresh": true,
      "reload_delay": 1000,
      "filter": "*.*"
    },
    "url_protocol": {
      "schemes": [
        "tebex-subs-sample-purchased"
      ]
    },
    "externally_connectable": {
      "matches": [
      "https://*.tebex.io"
      ]
    }
  }
}